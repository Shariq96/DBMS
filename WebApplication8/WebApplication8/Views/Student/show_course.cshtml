@model IEnumerable<WebApplication8.Models.StudentResult>

@{
    ViewBag.Title = "show_course";
}

<div style="padding:0px 50px; margin-top:-50px;">
    <h2>show_course</h2>


    <table class="table" style="padding:0px 450px 0px 200px">

        <tr>
            <th>
                @Html.DisplayNameFor(model => model.std_id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.std_name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.class_id)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.course_name)
            </th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.std_id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.std_name)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.class_id)
                </td>



                <td>
                    @Html.DisplayFor(modelItem => item.course_name)
                </td>

            </tr>
        }

    </table>

</div>